// JavaScript function to generate a random greeting:
function randomGreeting() {
  const greetings = ["Hello, world!", "Howdy, partner!", "Greetings, earthling!"];
  const randomIndex = Math.floor(Math.random() * greetings.length);
  return greetings[randomIndex];
}

console.log(randomGreeting()); // Output: (random greeting from the array)
